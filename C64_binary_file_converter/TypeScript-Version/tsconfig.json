{
  "compilerOptions": {
    "target": "ESNEXT",
    "module": "ESNEXT",
    "lib": [
      "ESNEXT",
      "DOM"
    ],
    "rootDir": "src",
    "outDir": "out",
    "sourceMap": true,
    "allowJs": true,
    "checkJs": false,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
  },
}